{
  "sx1509": {
    "schemas": {
      "CONFIG_SCHEMA": {
        "type": "schema",
        "schema": {
          "config_vars": {
            "id": {
              "key": "GeneratedID",
              "id_type": {
                "class": "sx1509::SX1509Component",
                "parents": [
                  "Component",
                  "i2c::I2CDevice"
                ]
              }
            },
            "keypad": {
              "key": "Optional",
              "type": "schema",
              "schema": {
                "config_vars": {
                  "key_rows": {
                    "key": "Required",
                    "type": "integer"
                  },
                  "key_columns": {
                    "key": "Required",
                    "type": "integer"
                  },
                  "sleep_time": {
                    "key": "Optional",
                    "type": "integer"
                  },
                  "scan_time": {
                    "key": "Optional",
                    "type": "integer"
                  },
                  "debounce_time": {
                    "key": "Optional",
                    "type": "integer"
                  }
                }
              }
            },
            "i2c_id": {
              "key": "GeneratedID",
              "use_id_type": "i2c::I2CBus",
              "type": "use_id"
            },
            "address": {
              "key": "Optional",
              "default": "62",
              "data_type": "hex_uint8_t"
            }
          },
          "extends": [
            "core.COMPONENT_SCHEMA"
          ]
        },
        "is_list": true
      }
    },
    "pin": {
      "type": "schema",
      "schema": {
        "extends": [
          "sx1509.SX1509_PIN_SCHEMA"
        ]
      }
    }
  },
  "sx1509.binary_sensor": {
    "schemas": {
      "CONFIG_SCHEMA": {
        "type": "schema",
        "schema": {
          "extends": [
            "binary_sensor.BINARY_SENSOR_SCHEMA"
          ],
          "config_vars": {
            "id": {
              "id_type": {
                "class": "sx1509::SX1509BinarySensor",
                "parents": [
                  "binary_sensor::BinarySensor",
                  "EntityBase"
                ]
              }
            },
            "sx1509_id": {
              "key": "GeneratedID",
              "use_id_type": "sx1509::SX1509Component",
              "type": "use_id"
            },
            "row": {
              "key": "Required",
              "type": "integer"
            },
            "col": {
              "key": "Required",
              "type": "integer"
            }
          }
        }
      }
    }
  },
  "sx1509.output": {
    "schemas": {
      "CONFIG_SCHEMA": {
        "type": "schema",
        "schema": {
          "extends": [
            "output.FLOAT_OUTPUT_SCHEMA",
            "core.COMPONENT_SCHEMA"
          ],
          "config_vars": {
            "id": {
              "key": "Required",
              "id_type": {
                "class": "sx1509::SX1509FloatOutputChannel",
                "parents": [
                  "output::FloatOutput",
                  "output::BinaryOutput",
                  "Component"
                ]
              }
            },
            "sx1509_id": {
              "key": "GeneratedID",
              "use_id_type": "sx1509::SX1509Component",
              "type": "use_id"
            },
            "pin": {
              "key": "Required",
              "type": "integer"
            }
          }
        }
      }
    }
  }
}