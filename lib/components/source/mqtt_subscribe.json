{
  "mqtt_subscribe.sensor": {
    "schemas": {
      "CONFIG_SCHEMA": {
        "type": "schema",
        "schema": {
          "extends": [
            "sensor.SENSOR_SCHEMA",
            "core.COMPONENT_SCHEMA"
          ],
          "config_vars": {
            "id": {
              "id_type": {
                "class": "mqtt_subscribe::MQTTSubscribeSensor",
                "parents": [
                  "sensor::Sensor",
                  "EntityBase",
                  "Component"
                ]
              }
            },
            "accuracy_decimals": {
              "default": "1"
            },
            "mqtt_parent_id": {
              "key": "GeneratedID",
              "use_id_type": "mqtt::MQTTClientComponent",
              "type": "use_id"
            },
            "topic": {
              "key": "Required"
            },
            "qos": {
              "key": "Optional",
              "default": "0"
            }
          }
        }
      }
    }
  },
  "mqtt_subscribe.text_sensor": {
    "schemas": {
      "CONFIG_SCHEMA": {
        "type": "schema",
        "schema": {
          "extends": [
            "text_sensor.TEXT_SENSOR_SCHEMA",
            "core.COMPONENT_SCHEMA"
          ],
          "config_vars": {
            "id": {
              "id_type": {
                "class": "mqtt_subscribe::MQTTSubscribeTextSensor",
                "parents": [
                  "text_sensor::TextSensor",
                  "EntityBase",
                  "Component"
                ]
              }
            },
            "mqtt_parent_id": {
              "key": "GeneratedID",
              "use_id_type": "mqtt::MQTTClientComponent",
              "type": "use_id"
            },
            "topic": {
              "key": "Required"
            },
            "qos": {
              "key": "Optional",
              "default": "0"
            }
          }
        }
      }
    }
  }
}