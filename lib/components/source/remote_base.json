{
  "remote_base": {
    "schemas": {
      "BASE_REMOTE_TRANSMITTER_SCHEMA": {
        "type": "schema",
        "schema": {
          "config_vars": {
            "transmitter_id": {
              "key": "GeneratedID",
              "use_id_type": "remote_base::RemoteTransmitterBase",
              "type": "use_id"
            },
            "repeat": {
              "key": "Optional"
            }
          }
        }
      },
      "CANALSAT_SCHEMA": {
        "type": "schema",
        "schema": {
          "config_vars": {
            "device": {
              "key": "Required",
              "data_type": "hex_uint8_t"
            },
            "address": {
              "key": "Optional",
              "default": "0",
              "data_type": "hex_uint8_t"
            },
            "command": {
              "key": "Required",
              "data_type": "hex_uint8_t"
            }
          }
        }
      },
      "RC_SWITCH_PROTOCOL_SCHEMA": {
        "type": "schema",
        "schema": {
          "config_vars": {
            "pulse_length": {
              "key": "Required",
              "type": "integer",
              "data_type": "uint32_t"
            },
            "sync": {
              "key": "Optional",
              "default": "[1, 31]",
              "data_type": "RC_SWITCH_TIMING_SCHEMA"
            },
            "zero": {
              "key": "Optional",
              "default": "[1, 3]",
              "data_type": "RC_SWITCH_TIMING_SCHEMA"
            },
            "one": {
              "key": "Optional",
              "default": "[3, 1]",
              "data_type": "RC_SWITCH_TIMING_SCHEMA"
            },
            "inverted": {
              "key": "Optional",
              "default": "False",
              "type": "boolean"
            }
          }
        }
      }
    },
    "dumper": {
      "canalsat": {},
      "canalsatld": {},
      "coolix": {},
      "dish": {},
      "jvc": {},
      "lg": {},
      "magiquest": {},
      "nec": {},
      "pioneer": {},
      "pronto": {},
      "sony": {},
      "raw": {},
      "drayton": {},
      "rc5": {},
      "rc6": {},
      "rc_switch": {},
      "samsung": {},
      "samsung36": {},
      "toshiba_ac": {},
      "panasonic": {},
      "nexa": {},
      "midea": {},
      "aeha": {}
    }
  }
}