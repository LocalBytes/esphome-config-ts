            },
            "switches": {
              "key": "Required",
              "is_list": true,
              "type": "schema",
              "schema": {
                "extends": [
                  "switch._SWITCH_SCHEMA"
                ],
                "config_vars": {
                  "restore_mode": {
                    "key": "Optional",
                    "default": "ALWAYS_OFF",
                    "type": "enum",
                    "values": {
                      "RESTORE_DEFAULT_OFF": null,
                      "RESTORE_DEFAULT_ON": null,
                      "ALWAYS_OFF": null,
                      "ALWAYS_ON": null,
                      "RESTORE_INVERTED_DEFAULT_OFF": null,
                      "RESTORE_INVERTED_DEFAULT_ON": null,
                      "DISABLED": null
                    }
                  },
                  "id": {
                    "key": "GeneratedID",
                    "id_type": {
                      "class": "switch_::Switch",
                      "parents": [
                        "EntityBase"
                      ]
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
