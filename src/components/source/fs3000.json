{
  "fs3000.sensor": {
    "schemas": {
      "CONFIG_SCHEMA": {
        "type": "schema",
        "schema": {
          "extends": [
            "sensor.SENSOR_SCHEMA",
            "core.COMPONENT_SCHEMA"
          ],
          "config_vars": {
            "id": {
              "id_type": {
                "class": "fs3000::FS3000Component",
                "parents": [
                  "PollingComponent",
                  "Component",
                  "i2c::I2CDevice",
                  "sensor::Sensor",
                  "EntityBase"
                ]
              }
            },
            "unit_of_measurement": {
              "default": "m/s"
            },
            "accuracy_decimals": {
              "default": "2"
            },
            "device_class": {
              "default": "wind_speed"
            },
            "state_class": {
              "default": "measurement"
            },
            "model": {
              "key": "Required",
              "type": "enum",
              "values": {
                "1005": null,
                "1015": null
              }
            },
            "update_interval": {
              "key": "Optional",
              "default": "60s"
            },
            "i2c_id": {
              "key": "GeneratedID",
              "use_id_type": "i2c::I2CBus",
              "type": "use_id"
            },
            "address": {
              "key": "Optional",
              "default": "40",
              "data_type": "hex_uint8_t"
            }
          }
        }
      }
    }
  }
}