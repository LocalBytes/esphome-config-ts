      "DELTA_SCHEMA": {
        "type": "schema",
        "schema": {
          "config_vars": {
            "value": {
              "key": "Required",
              "data_type": "positive_float"
            },
            "type": {
              "key": "Optional",
              "default": "absolute",
              "type": "enum",
              "values": {
                "absolute": null,
                "percentage": null
              }
            }
          }
        }
      },
      "TIMEOUT_SCHEMA": {
        "type": "schema",
        "maybe": "timeout",
        "schema": {
          "config_vars": {
            "timeout": {
              "key": "Required",
              "type": "schema",
              "schema": {
                "extends": [
                  "core.positive_time_period_milliseconds"
                ]
              }
            },
            "value": {
              "key": "Optional",
              "default": "nan"
            }
          }
        }
      },
      "CLAMP_SCHEMA": {
        "type": "schema",
        "schema": {
          "config_vars": {
            "min_value": {
              "key": "Optional",
