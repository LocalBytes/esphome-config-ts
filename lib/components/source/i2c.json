{
  "i2c": {
    "schemas": {
      "CONFIG_SCHEMA": {
        "type": "schema",
        "schema": {
          "config_vars": {
            "id": {
              "key": "GeneratedID",
              "id_type": {
                "class": "i2c::I2CBus",
                "parents": [
                  "Component"
                ]
              }
            },
            "sda": {
              "key": "Optional",
              "default": "SDA",
              "data_type": "pin_with_input_and_output_support"
            },
            "sda_pullup_enabled": {
              "key": "Optional",
              "type": "boolean"
            },
            "scl": {
              "key": "Optional",
              "default": "SCL",
              "data_type": "pin_with_input_and_output_support"
            },
            "scl_pullup_enabled": {
              "key": "Optional",
              "type": "boolean"
            },
            "frequency": {
              "key": "Optional",
              "default": "50kHz"
            },
            "scan": {
              "key": "Optional",
              "default": "True",
              "type": "boolean"
            }
          },
          "extends": [
            "core.COMPONENT_SCHEMA"
          ]
        },
        "is_list": true
      }
    }
  }
}