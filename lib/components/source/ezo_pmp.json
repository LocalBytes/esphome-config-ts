      "CONFIG_SCHEMA": {
        "type": "schema",
        "schema": {
          "config_vars": {
            "id": {
              "key": "GeneratedID",
              "use_id_type": "ezo_pmp::EzoPMP",
              "type": "use_id"
            },
            "dosing_mode": {
              "key": "Optional",
              "type": "schema",
              "schema": {
                "extends": [
                  "text_sensor.TEXT_SENSOR_SCHEMA"
                ],
                "config_vars": {
                  "entity_category": {
                    "default": ""
                  }
                }
              }
            },
            "calibration_status": {
              "key": "Optional",
              "type": "schema",
              "schema": {
                "extends": [
                  "text_sensor.TEXT_SENSOR_SCHEMA"
                ],
                "config_vars": {
                  "entity_category": {
                    "default": "diagnostic"
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}