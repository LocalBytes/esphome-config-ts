{
  "hydreon_rgxx.sensor": {
    "schemas": {
      "CONFIG_SCHEMA": {
        "type": "schema",
        "schema": {
          "config_vars": {
            "id": {
              "key": "GeneratedID",
              "id_type": {
                "class": "hydreon_rgxx::HydreonRGxxComponent",
                "parents": [
                  "PollingComponent",
                  "Component",
                  "uart::UARTDevice"
                ]
              }
            },
            "model": {
              "key": "Required",
              "type": "enum",
              "values": {
                "RG_9": null,
                "RG_15": null
              }
            },
            "acc": {
              "key": "Optional",
              "type": "schema",
              "schema": {
                "extends": [
                  "sensor.SENSOR_SCHEMA"
                ],
                "config_vars": {
                  "unit_of_measurement": {
                    "default": "mm"
                  },
                  "accuracy_decimals": {
                    "default": "2"
                  },
                  "device_class": {
                    "default": "precipitation"
                  },
                  "state_class": {
                    "default": "measurement"
                  }
                }
              }
            },
            "event_acc": {
              "key": "Optional",
              "type": "schema",
              "schema": {
                "extends": [
                  "sensor.SENSOR_SCHEMA"
                ],
                "config_vars": {
                  "unit_of_measurement": {
                    "default": "mm"
                  },
                  "accuracy_decimals": {
                    "default": "2"
                  },
                  "device_class": {
                    "default": "precipitation"
                  },
                  "state_class": {
                    "default": "measurement"
                  }
                }
              }
            },
            "total_acc": {
              "key": "Optional",
              "type": "schema",
              "schema": {
                "extends": [
                  "sensor.SENSOR_SCHEMA"
                ],
                "config_vars": {
                  "unit_of_measurement": {
                    "default": "mm"
                  },
                  "accuracy_decimals": {
                    "default": "2"
                  },
                  "device_class": {
                    "default": "precipitation"
                  },
                  "state_class": {
                    "default": "total_increasing"
                  }
                }
              }
            },
            "r_int": {
              "key": "Optional",
              "type": "schema",
              "schema": {
                "extends": [
                  "sensor.SENSOR_SCHEMA"
                ],
                "config_vars": {
                  "unit_of_measurement": {
                    "default": "mm/h"
                  },
                  "accuracy_decimals": {
                    "default": "2"
                  },
                  "device_class": {
                    "default": "precipitation_intensity"
                  },
                  "state_class": {
                    "default": "measurement"
                  }
                }
              }
            },
            "moisture": {
              "key": "Optional",
              "type": "schema",
              "schema": {
                "extends": [
                  "sensor.SENSOR_SCHEMA"
                ],
                "config_vars": {
                  "unit_of_measurement": {
                    "default": "intensity"
                  },
                  "accuracy_decimals": {
                    "default": "0"
                  },
                  "device_class": {
                    "default": "precipitation_intensity"
                  },
                  "state_class": {
                    "default": "measurement"
                  }
                }
              }
            },
            "temperature": {
              "key": "Optional",
              "type": "schema",
              "schema": {
                "extends": [
                  "sensor.SENSOR_SCHEMA"
                ],
                "config_vars": {
                  "unit_of_measurement": {
                    "default": "\u00b0C"
                  },
                  "icon": {
                    "default": "mdi:thermometer"
                  },
                  "accuracy_decimals": {
                    "default": "0"
                  },
                  "state_class": {
                    "default": "measurement"
                  }
                }
              }
            },
            "update_interval": {
              "key": "Optional",
              "default": "60s"
            },
            "uart_id": {
              "key": "GeneratedID",
              "use_id_type": "uart::UARTComponent",
              "type": "use_id"
            }
          },
          "extends": [
            "core.COMPONENT_SCHEMA"
          ]
        }
      }
    }
  },
  "hydreon_rgxx.binary_sensor": {
    "schemas": {
      "CONFIG_SCHEMA": {
        "type": "schema",
        "schema": {
          "config_vars": {
            "id": {
              "key": "GeneratedID",
              "id_type": {
                "class": "hydreon_rgxx::HydreonRGxxBinaryComponent",
                "parents": [
                  "Component"
                ]
              }
            },
            "hydreon_rgxx_id": {
              "key": "GeneratedID",
              "use_id_type": "hydreon_rgxx::HydreonRGxxComponent",
              "type": "use_id"
            },
            "too_cold": {
              "key": "Optional",
              "type": "schema",
              "schema": {
                "extends": [
                  "binary_sensor.BINARY_SENSOR_SCHEMA"
                ],
                "config_vars": {
                  "device_class": {
                    "default": "cold"
                  }
                }
              }
            },
            "lens_bad": {
              "key": "Optional",
              "type": "schema",
              "schema": {
                "extends": [
                  "binary_sensor.BINARY_SENSOR_SCHEMA"
                ],
                "config_vars": {
                  "device_class": {
                    "default": "problem"
                  }
                }
              }
            },
            "em_sat": {
              "key": "Optional",
              "type": "schema",
              "schema": {
                "extends": [
                  "binary_sensor.BINARY_SENSOR_SCHEMA"
                ],
                "config_vars": {
                  "device_class": {
                    "default": "problem"
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}