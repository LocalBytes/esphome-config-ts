                "parents": [
                  "Component"
                ]
              }
            },
            "type": {
              "key": "Required",
              "type": "enum",
              "values": {
                "LAN8720": null,
                "RTL8201": null,
                "DP83848": null,
                "IP101": null,
                "JL1101": null,
                "KSZ8081": null,
                "KSZ8081RNA": null
              }
            },
            "mdc_pin": {
              "key": "Required",
              "internal": true,
              "modes": [
                "output"
              ],
              "type": "pin"
            },
            "mdio_pin": {
              "key": "Required",
              "internal": true,
              "modes": [
                "output"
              ],
              "type": "pin"
            },
            "clk_mode": {
              "key": "Optional",
              "default": "GPIO0_IN",
