                  },
                  "dns2": {
                    "key": "Optional",
                    "default": "0.0.0.0"
                  }
                }
              }
            },
            "domain": {
              "key": "Optional",
              "default": ".local"
            },
            "use_address": {
              "key": "Optional",
              "type": "string"
