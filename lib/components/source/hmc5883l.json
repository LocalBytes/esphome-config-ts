{
  "hmc5883l.sensor": {
    "schemas": {
      "field_strength_schema": {
        "type": "schema",
        "schema": {
          "extends": [
            "sensor.SENSOR_SCHEMA"
          ],
          "config_vars": {
            "unit_of_measurement": {
              "default": "\u00b5T"
            },
            "icon": {
              "default": "mdi:magnet"
            },
            "accuracy_decimals": {
              "default": "1"
            },
            "state_class": {
              "default": "measurement"
            }
          }
        }
      },
      "CONFIG_SCHEMA": {
        "type": "schema",
        "schema": {
          "config_vars": {
            "id": {
              "key": "GeneratedID",
              "id_type": {
                "class": "hmc5883l::HMC5883LComponent",
                "parents": [
                  "PollingComponent",
                  "Component",
                  "i2c::I2CDevice"
                ]
              }
            },
            "address": {
              "key": "Optional",
              "default": "30",
              "data_type": "hex_uint8_t"
            },
            "oversampling": {
              "key": "Optional",
              "default": "1x"
            },
            "range": {
              "key": "Optional",
              "default": "130\u00b5T"
            },
            "field_strength_x": {
              "key": "Optional",
              "type": "schema",
              "schema": {
                "extends": [
                  "hmc5883l.sensor.field_strength_schema"
                ]
              }
            },
            "field_strength_y": {
              "key": "Optional",
              "type": "schema",
              "schema": {
                "extends": [
                  "hmc5883l.sensor.field_strength_schema"
                ]
              }
            },
            "field_strength_z": {
              "key": "Optional",
              "type": "schema",
              "schema": {
                "extends": [
                  "hmc5883l.sensor.field_strength_schema"
                ]
              }
            },
            "heading": {
              "key": "Optional",
              "type": "schema",
              "schema": {
                "extends": [
                  "sensor.SENSOR_SCHEMA"
                ],
                "config_vars": {
                  "unit_of_measurement": {
                    "default": "\u00b0"
                  },
                  "icon": {
                    "default": "mdi:screen-rotation"
                  },
                  "accuracy_decimals": {
                    "default": "1"
                  }
                }
              }
            },
            "update_interval": {
              "key": "Optional",
              "default": "60s"
            },
            "i2c_id": {
              "key": "GeneratedID",
              "use_id_type": "i2c::I2CBus",
              "type": "use_id"
            }
          },
          "extends": [
            "core.COMPONENT_SCHEMA"
          ]
        }
      }
    }
  }
}