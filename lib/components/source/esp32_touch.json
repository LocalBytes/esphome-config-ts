{
  "esp32_touch": {
    "schemas": {
      "CONFIG_SCHEMA": {
        "type": "schema",
        "schema": {
          "config_vars": {
            "id": {
              "key": "GeneratedID",
              "id_type": {
                "class": "esp32_touch::ESP32TouchComponent",
                "parents": [
                  "Component"
                ]
              }
            },
            "setup_mode": {
              "key": "Optional",
              "default": "False",
              "type": "boolean"
            },
            "sleep_duration": {
              "key": "Optional",
              "default": "27306us",
              "type": "schema",
              "schema": {
                "extends": [
                  "core.time_period_dict"
                ]
              }
            },
            "measurement_duration": {
              "key": "Optional",
              "default": "8192us",
              "type": "schema",
              "schema": {
                "extends": [
                  "core.time_period_dict"
                ]
              }
            },
            "low_voltage_reference": {
              "key": "Optional",
              "default": "0.5V"
            },
            "high_voltage_reference": {
              "key": "Optional",
              "default": "2.7V"
            },
            "voltage_attenuation": {
              "key": "Optional",
              "default": "0V"
            },
            "iir_filter": {
              "key": "Optional",
              "type": "schema",
              "schema": {
                "extends": [
                  "core.positive_time_period_milliseconds"
                ]
              }
            },
            "debounce_count": {
              "key": "Optional",
              "type": "integer"
            },
            "filter_mode": {
              "key": "Optional",
              "type": "enum",
              "values": {
                "IIR_4": null,
                "IIR_8": null,
                "IIR_16": null,
                "IIR_32": null,
                "IIR_64": null,
                "IIR_128": null,
                "IIR_256": null,
                "JITTER": null
              }
            },
            "noise_threshold": {
              "key": "Optional",
              "type": "integer"
            },
            "jitter_step": {
              "key": "Optional",
              "type": "integer"
            },
            "smooth_mode": {
              "key": "Optional",
              "type": "enum",
              "values": {
                "OFF": null,
                "IIR_2": null,
                "IIR_4": null,
                "IIR_8": null
              }
            },
            "denoise_grade": {
              "key": "Optional",
              "type": "enum",
              "values": {
                "BIT12": null,
                "BIT10": null,
                "BIT8": null,
                "BIT4": null
              }
            },
            "denoise_cap_level": {
              "key": "Optional",
              "type": "enum",
              "values": {
                "L0": null,
                "L1": null,
                "L2": null,
                "L3": null,
                "L4": null,
                "L5": null,
                "L6": null,
                "L7": null
              }
            },
            "waterproof_guard_ring": {
              "key": "Optional"
            },
            "waterproof_shield_driver": {
              "key": "Optional",
              "type": "enum",
              "values": {
                "L0": null,
                "L1": null,
                "L2": null,
                "L3": null,
                "L4": null,
                "L5": null,
                "L6": null,
                "L7": null
              }
            }
          },
          "extends": [
            "core.COMPONENT_SCHEMA"
          ]
        }
      }
    }
  },
  "esp32_touch.binary_sensor": {
    "schemas": {
      "CONFIG_SCHEMA": {
        "type": "schema",
        "schema": {
          "extends": [
            "binary_sensor.BINARY_SENSOR_SCHEMA"
          ],
          "config_vars": {
            "id": {
              "id_type": {
                "class": "esp32_touch::ESP32TouchBinarySensor",
                "parents": [
                  "binary_sensor::BinarySensor",
                  "EntityBase"
                ]
              }
            },
            "esp32_touch_id": {
              "key": "GeneratedID",
              "use_id_type": "esp32_touch::ESP32TouchComponent",
              "type": "use_id"
            },
            "pin": {
              "key": "Required"
            },
            "threshold": {
              "key": "Required",
              "type": "integer",
              "data_type": "uint32_t"
            },
            "wakeup_threshold": {
              "key": "Optional",
              "default": "0",
              "type": "integer",
              "data_type": "uint32_t"
            }
          }
        }
      }
    }
  }
}