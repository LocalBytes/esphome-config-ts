{
  "bme680_bsec": {
    "schemas": {
      "CONFIG_SCHEMA": {
        "type": "schema",
        "schema": {
          "config_vars": {
            "id": {
              "key": "GeneratedID",
              "id_type": {
                "class": "bme680_bsec::BME680BSECComponent",
                "parents": [
                  "Component",
                  "i2c::I2CDevice"
                ]
              }
            },
            "temperature_offset": {
              "key": "Optional",
              "default": "0"
            },
            "iaq_mode": {
              "key": "Optional",
              "default": "STATIC",
              "type": "enum",
              "values": {
                "STATIC": null,
                "MOBILE": null
              }
            },
            "sample_rate": {
              "key": "Optional",
              "default": "LP",
              "type": "enum",
              "values": {
                "LP": null,
                "ULP": null
              }
            },
            "state_save_interval": {
              "key": "Optional",
              "default": "6hours",
              "type": "schema",
              "schema": {
                "extends": [
                  "core.positive_time_period_minutes"
                ]
              }
            },
            "i2c_id": {
              "key": "GeneratedID",
              "use_id_type": "i2c::I2CBus",
              "type": "use_id"
            },
            "address": {
              "key": "Optional",
              "default": "118",
              "data_type": "hex_uint8_t"
            }
          }
        },
        "is_list": true
      }
    }
  },
  "bme680_bsec.sensor": {
