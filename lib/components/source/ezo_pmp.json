{
  "ezo_pmp": {
    "schemas": {
      "CONFIG_SCHEMA": {
        "type": "schema",
        "schema": {
          "config_vars": {
            "id": {
              "key": "GeneratedID",
              "id_type": {
                "class": "ezo_pmp::EzoPMP",
                "parents": [
                  "PollingComponent",
                  "Component",
                  "i2c::I2CDevice"
                ]
              }
            },
            "update_interval": {
              "key": "Optional",
              "default": "60s"
            },
            "i2c_id": {
              "key": "GeneratedID",
              "use_id_type": "i2c::I2CBus",
              "type": "use_id"
            },
            "address": {
              "key": "Optional",
              "default": "103",
              "data_type": "hex_uint8_t"
            }
          },
          "extends": [
            "core.COMPONENT_SCHEMA"
          ]
        },
        "is_list": true
      },
      "EZO_PMP_NO_ARGS_ACTION_SCHEMA": {
        "type": "schema",
        "maybe": "id",
        "schema": {
          "config_vars": {
            "id": {
              "key": "Required",
              "use_id_type": "ezo_pmp::EzoPMP",
              "type": "use_id"
            }
          }
        }
      }
    },
    "action": {
      "find": {
        "type": "schema",
        "schema": {
          "extends": [
            "ezo_pmp.EZO_PMP_NO_ARGS_ACTION_SCHEMA"
          ]
        }
      },
      "dose_continuously": {
        "type": "schema",
        "schema": {
          "extends": [
            "ezo_pmp.EZO_PMP_NO_ARGS_ACTION_SCHEMA"
          ]
        }
      },
      "clear_total_volume_dosed": {
        "type": "schema",
        "schema": {
          "extends": [
            "ezo_pmp.EZO_PMP_NO_ARGS_ACTION_SCHEMA"
          ]
        }
      },
      "clear_calibration": {
        "type": "schema",
        "schema": {
          "extends": [
            "ezo_pmp.EZO_PMP_NO_ARGS_ACTION_SCHEMA"
          ]
        }
      },
      "pause_dosing": {
        "type": "schema",
        "schema": {
          "extends": [
            "ezo_pmp.EZO_PMP_NO_ARGS_ACTION_SCHEMA"
          ]
        }
      },
      "stop_dosing": {
        "type": "schema",
        "schema": {
          "extends": [
            "ezo_pmp.EZO_PMP_NO_ARGS_ACTION_SCHEMA"
          ]
        }
      },
      "dose_volume": {
        "type": "schema",
        "schema": {
          "config_vars": {
            "id": {
              "key": "Required",
              "use_id_type": "ezo_pmp::EzoPMP",
              "type": "use_id"
            },
            "volume": {
              "key": "Required",
              "templatable": true
            }
          }
        }
      },
      "dose_volume_over_time": {
        "type": "schema",
        "schema": {
          "config_vars": {
            "id": {
              "key": "Required",
              "use_id_type": "ezo_pmp::EzoPMP",
              "type": "use_id"
            },
            "volume": {
              "key": "Required",
              "templatable": true
            },
            "duration": {
              "key": "Required",
              "templatable": true,
              "type": "integer"
            }
          }
        }
      },
      "dose_with_constant_flow_rate": {
        "type": "schema",
        "schema": {
          "config_vars": {
            "id": {
              "key": "Required",
              "use_id_type": "ezo_pmp::EzoPMP",
              "type": "use_id"
            },
            "volume_per_minute": {
              "key": "Required",
              "templatable": true
            },
            "duration": {
              "key": "Required",
              "templatable": true,
              "type": "integer"
            }
          }
        }
      },
      "set_calibration_volume": {
        "type": "schema",
        "schema": {
          "config_vars": {
            "id": {
              "key": "Required",
              "use_id_type": "ezo_pmp::EzoPMP",
              "type": "use_id"
            },
            "volume": {
              "key": "Required",
              "templatable": true
            }
          }
        }
      },
      "change_i2c_address": {
        "type": "schema",
        "schema": {
          "config_vars": {
            "id": {
              "key": "Required",
              "use_id_type": "ezo_pmp::EzoPMP",
              "type": "use_id"
            },
            "address": {
              "key": "Required",
              "templatable": true,
              "type": "integer"
            }
          }
        }
      },
      "arbitrary_command": {
        "type": "schema",
        "schema": {
          "config_vars": {
            "id": {
              "key": "Required",
              "use_id_type": "ezo_pmp::EzoPMP",
              "type": "use_id"
            },
            "command": {
              "key": "Required",
              "templatable": true,
              "type": "string"
            }
          }
        }
      }
    }
  },
  "ezo_pmp.sensor": {
    "schemas": {
      "CONFIG_SCHEMA": {
        "type": "schema",
        "schema": {
          "config_vars": {
            "id": {
              "key": "GeneratedID",
              "use_id_type": "ezo_pmp::EzoPMP",
              "type": "use_id"
            },
            "current_volume_dosed": {
              "key": "Optional",
              "type": "schema",
              "schema": {
                "extends": [
                  "sensor.SENSOR_SCHEMA"
                ],
                "config_vars": {
                  "unit_of_measurement": {
                    "default": "ml"
                  },
                  "accuracy_decimals": {
                    "default": "2"
                  },
                  "device_class": {
                    "default": ""
                  },
                  "state_class": {
                    "default": "measurement"
                  },
                  "entity_category": {
                    "default": ""
                  }
                }
              }
            },
            "last_volume_requested": {
              "key": "Optional",
              "type": "schema",
              "schema": {
                "extends": [
                  "sensor.SENSOR_SCHEMA"
                ],
                "config_vars": {
                  "unit_of_measurement": {
                    "default": "ml"
                  },
                  "accuracy_decimals": {
                    "default": "2"
                  },
                  "device_class": {
                    "default": ""
                  },
                  "state_class": {
                    "default": "measurement"
                  },
                  "entity_category": {
                    "default": ""
                  }
                }
              }
            },
            "max_flow_rate": {
              "key": "Optional",
              "type": "schema",
              "schema": {
                "extends": [
                  "sensor.SENSOR_SCHEMA"
                ],
                "config_vars": {
                  "unit_of_measurement": {
                    "default": "ml/min"
                  },
                  "accuracy_decimals": {
                    "default": "2"
                  },
                  "device_class": {
                    "default": ""
                  },
                  "state_class": {
                    "default": ""
                  },
                  "entity_category": {
                    "default": "diagnostic"
                  }
                }
              }
            },
            "total_volume_dosed": {
              "key": "Optional",
              "type": "schema",
              "schema": {
                "extends": [
                  "sensor.SENSOR_SCHEMA"
                ],
                "config_vars": {
                  "unit_of_measurement": {
                    "default": "ml"
                  },
                  "accuracy_decimals": {
                    "default": "2"
                  },
                  "device_class": {
                    "default": ""
                  },
                  "state_class": {
                    "default": "measurement"
                  },
                  "entity_category": {
                    "default": "diagnostic"
                  }
                }
              }
            },
            "absolute_total_volume_dosed": {
              "key": "Optional",
              "type": "schema",
              "schema": {
                "extends": [
                  "sensor.SENSOR_SCHEMA"
                ],
                "config_vars": {
                  "unit_of_measurement": {
                    "default": "ml"
                  },
                  "accuracy_decimals": {
                    "default": "2"
                  },
                  "device_class": {
                    "default": ""
                  },
                  "state_class": {
                    "default": "measurement"
                  },
                  "entity_category": {
                    "default": "diagnostic"
                  }
                }
              }
            },
            "pump_voltage": {
              "key": "Optional",
              "type": "schema",
              "schema": {
                "extends": [
                  "sensor.SENSOR_SCHEMA"
                ],
                "config_vars": {
                  "unit_of_measurement": {
                    "default": "V"
                  },
                  "accuracy_decimals": {
                    "default": "2"
                  },
                  "device_class": {
                    "default": "voltage"
                  },
                  "state_class": {
                    "default": "measurement"
                  },
                  "entity_category": {
                    "default": "diagnostic"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "ezo_pmp.binary_sensor": {
    "schemas": {
      "CONFIG_SCHEMA": {
        "type": "schema",
        "schema": {
          "config_vars": {
            "id": {
              "key": "GeneratedID",
              "use_id_type": "ezo_pmp::EzoPMP",
              "type": "use_id"
            },
            "pump_state": {
              "key": "Optional",
              "type": "schema",
              "schema": {
                "extends": [
                  "binary_sensor.BINARY_SENSOR_SCHEMA"
                ],
                "config_vars": {
                  "entity_category": {
                    "default": ""
                  },
                  "device_class": {
                    "default": "running"
                  }
                }
              }
            },
            "is_paused": {
              "key": "Optional",
              "type": "schema",
              "schema": {
                "extends": [
                  "binary_sensor.BINARY_SENSOR_SCHEMA"
                ],
                "config_vars": {
                  "entity_category": {
                    "default": ""
                  },
                  "device_class": {
                    "default": ""
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "ezo_pmp.text_sensor": {
