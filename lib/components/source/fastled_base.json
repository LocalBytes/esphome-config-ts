{
  "fastled_base": {
    "schemas": {
      "BASE_SCHEMA": {
        "type": "schema",
        "schema": {
          "extends": [
            "light.ADDRESSABLE_LIGHT_SCHEMA",
            "core.COMPONENT_SCHEMA"
          ],
          "config_vars": {
            "output_id": {
              "key": "GeneratedID",
              "id_type": {
                "class": "fastled_base::FastLEDLightOutput",
                "parents": [
                  "light::AddressableLight",
                  "light::LightOutput",
                  "Component"
                ]
              }
            },
            "num_leds": {
              "key": "Required",
              "type": "integer",
              "data_type": "positive_not_null_int"
            },
            "rgb_order": {
              "key": "Optional",
              "type": "enum",
              "values": {
                "RGB": null,
                "RBG": null,
                "GRB": null,
                "GBR": null,
                "BRG": null,
                "BGR": null
              }
            },
            "max_refresh_rate": {
              "key": "Optional",
              "type": "schema",
              "schema": {
                "extends": [
                  "core.positive_time_period_microseconds"
                ]
              }
            }
          }
        }
      }
    }
  }
}