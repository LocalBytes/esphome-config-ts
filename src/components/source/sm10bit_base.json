{
  "sm10bit_base": {
    "schemas": {
      "SM10BIT_BASE_CONFIG_SCHEMA": {
        "type": "schema",
        "schema": {
          "config_vars": {
            "data_pin": {
              "key": "Required",
              "schema": true,
              "modes": [
                "output"
              ],
              "type": "pin"
            },
            "clock_pin": {
              "key": "Required",
              "schema": true,
              "modes": [
                "output"
              ],
              "type": "pin"
            },
            "max_power_color_channels": {
              "key": "Optional",
              "default": "2",
              "type": "integer"
            },
            "max_power_white_channels": {
              "key": "Optional",
              "default": "4",
              "type": "integer"
            }
          },
          "extends": [
            "core.COMPONENT_SCHEMA"
          ]
        }
      }
    }
  }
}