      "EXPONENTIAL_AVERAGE_SCHEMA": {
        "type": "schema",
        "schema": {
          "config_vars": {
            "alpha": {
              "key": "Optional",
              "default": "0.1",
              "data_type": "positive_float"
            },
            "send_every": {
              "key": "Optional",
              "default": "15",
              "type": "integer",
              "data_type": "positive_not_null_int"
            },
            "send_first_at": {
              "key": "Optional",
              "default": "1",
              "type": "integer",
              "data_type": "positive_not_null_int"
            }
          }
