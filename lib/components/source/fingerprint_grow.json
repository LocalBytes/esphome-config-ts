{
  "fingerprint_grow": {
    "schemas": {
      "CONFIG_SCHEMA": {
        "type": "schema",
        "schema": {
          "config_vars": {
            "id": {
              "key": "GeneratedID",
              "id_type": {
                "class": "fingerprint_grow::FingerprintGrowComponent",
                "parents": [
                  "PollingComponent",
                  "Component",
                  "uart::UARTDevice"
                ]
              }
            },
            "sensing_pin": {
              "key": "Optional",
              "schema": true,
              "modes": [
                "input"
              ],
              "type": "pin"
            },
            "password": {
              "key": "Optional",
              "type": "integer",
              "data_type": "uint32_t"
            },
            "new_password": {
              "key": "Optional",
              "type": "integer",
              "data_type": "uint32_t"
            },
            "on_finger_scan_matched": {
              "key": "Optional",
              "type": "trigger"
            },
            "on_finger_scan_unmatched": {
              "key": "Optional",
              "type": "trigger"
            },
            "on_enrollment_scan": {
              "key": "Optional",
              "type": "trigger"
            },
            "on_enrollment_done": {
              "key": "Optional",
              "type": "trigger"
            },
            "on_enrollment_failed": {
              "key": "Optional",
              "type": "trigger"
            },
            "update_interval": {
              "key": "Optional",
              "default": "500ms"
            },
            "uart_id": {
              "key": "GeneratedID",
              "use_id_type": "uart::UARTComponent",
              "type": "use_id"
            }
          },
          "extends": [
            "core.COMPONENT_SCHEMA"
          ]
        },
        "is_list": true
      }
    },
    "action": {
      "enroll": {
        "type": "schema",
        "maybe": "finger_id",
        "schema": {
          "config_vars": {
            "id": {
              "key": "GeneratedID",
              "use_id_type": "fingerprint_grow::FingerprintGrowComponent",
              "type": "use_id"
            },
            "finger_id": {
              "key": "Required",
              "templatable": true,
              "type": "integer",
              "data_type": "uint16_t"
            },
            "num_scans": {
              "key": "Optional",
              "templatable": true,
              "type": "integer",
              "data_type": "uint8_t"
            }
          }
        }
      },
      "cancel_enroll": {
        "type": "schema",
        "schema": {
          "config_vars": {
            "id": {
              "key": "GeneratedID",
              "use_id_type": "fingerprint_grow::FingerprintGrowComponent",
              "type": "use_id"
            }
          }
        }
      },
      "delete": {
        "type": "schema",
        "maybe": "finger_id",
        "schema": {
          "config_vars": {
            "id": {
              "key": "GeneratedID",
              "use_id_type": "fingerprint_grow::FingerprintGrowComponent",
              "type": "use_id"
            },
            "finger_id": {
              "key": "Required",
              "templatable": true,
              "type": "integer",
              "data_type": "uint16_t"
            }
          }
        }
      },
      "delete_all": {
        "type": "schema",
        "schema": {
          "config_vars": {
            "id": {
              "key": "GeneratedID",
              "use_id_type": "fingerprint_grow::FingerprintGrowComponent",
              "type": "use_id"
            }
          }
        }
      },
      "led_control": {
        "type": "schema",
        "schema": {
          "config_vars": {
            "id": {
              "key": "GeneratedID",
              "use_id_type": "fingerprint_grow::FingerprintGrowComponent",
              "type": "use_id"
            },
            "state": {
              "key": "Required",
              "templatable": true,
              "type": "boolean"
            }
          }
        },
        "maybe": "state"
      },
      "aura_led_control": {
        "type": "schema",
        "schema": {
          "config_vars": {
            "id": {
              "key": "GeneratedID",
              "use_id_type": "fingerprint_grow::FingerprintGrowComponent",
              "type": "use_id"
            },
            "state": {
              "key": "Required",
              "templatable": true,
              "type": "enum",
              "values": {
                "BREATHING": null,
                "FLASHING": null,
                "ALWAYS_ON": null,
                "ALWAYS_OFF": null,
                "GRADUAL_ON": null,
                "GRADUAL_OFF": null
              }
            },
            "speed": {
              "key": "Required",
              "templatable": true,
              "type": "integer",
              "data_type": "uint8_t"
            },
            "color": {
              "key": "Required",
              "templatable": true,
              "type": "enum",
              "values": {
                "RED": null,
                "BLUE": null,
                "PURPLE": null,
                "GREEN": null,
                "YELLOW": null,
                "CYAN": null,
                "WHITE": null
              }
            },
            "count": {
              "key": "Required",
              "templatable": true,
              "type": "integer",
              "data_type": "uint8_t"
            }
          }
        }
      }
    }
  },
  "fingerprint_grow.sensor": {
    "schemas": {
      "CONFIG_SCHEMA": {
        "type": "schema",
        "schema": {
          "config_vars": {
            "fingerprint_grow_id": {
              "key": "GeneratedID",
              "use_id_type": "fingerprint_grow::FingerprintGrowComponent",
              "type": "use_id"
            },
            "fingerprint_count": {
              "key": "Optional",
              "type": "schema",
              "schema": {
                "extends": [
                  "sensor.SENSOR_SCHEMA"
                ],
                "config_vars": {
                  "icon": {
                    "default": "mdi:fingerprint"
                  },
                  "accuracy_decimals": {
                    "default": "0"
                  },
                  "entity_category": {
                    "default": "diagnostic"
                  }
                }
              }
            },
            "status": {
              "key": "Optional",
              "type": "schema",
              "schema": {
                "extends": [
                  "sensor.SENSOR_SCHEMA"
                ],
                "config_vars": {
                  "accuracy_decimals": {
                    "default": "0"
                  },
                  "entity_category": {
                    "default": "diagnostic"
                  }
                }
              }
            },
            "capacity": {
              "key": "Optional",
              "type": "schema",
              "schema": {
                "extends": [
                  "sensor.SENSOR_SCHEMA"
                ],
                "config_vars": {
                  "icon": {
                    "default": "mdi:database"
                  },
                  "accuracy_decimals": {
                    "default": "0"
                  },
                  "entity_category": {
                    "default": "diagnostic"
                  }
                }
              }
            },
            "security_level": {
              "key": "Optional",
              "type": "schema",
              "schema": {
                "extends": [
                  "sensor.SENSOR_SCHEMA"
                ],
                "config_vars": {
                  "icon": {
                    "default": "mdi:security"
                  },
                  "accuracy_decimals": {
                    "default": "0"
                  },
                  "entity_category": {
                    "default": "diagnostic"
                  }
                }
              }
            },
            "last_finger_id": {
              "key": "Optional",
              "type": "schema",
              "schema": {
                "extends": [
                  "sensor.SENSOR_SCHEMA"
                ],
                "config_vars": {
                  "icon": {
                    "default": "mdi:account"
                  },
                  "accuracy_decimals": {
                    "default": "0"
                  },
                  "entity_category": {
                    "default": "diagnostic"
                  }
                }
              }
            },
            "last_confidence": {
              "key": "Optional",
              "type": "schema",
              "schema": {
                "extends": [
                  "sensor.SENSOR_SCHEMA"
                ],
                "config_vars": {
                  "icon": {
                    "default": "mdi:account-check"
                  },
                  "accuracy_decimals": {
                    "default": "0"
                  },
                  "entity_category": {
                    "default": "diagnostic"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "fingerprint_grow.binary_sensor": {
    "schemas": {
      "CONFIG_SCHEMA": {
        "type": "schema",
        "schema": {
          "extends": [
            "binary_sensor.BINARY_SENSOR_SCHEMA"
          ],
          "config_vars": {
            "fingerprint_grow_id": {
              "key": "GeneratedID",
              "use_id_type": "fingerprint_grow::FingerprintGrowComponent",
              "type": "use_id"
            },
            "icon": {
              "default": "mdi:key-plus"
            }
          }
        }
      }
    }
  }
}