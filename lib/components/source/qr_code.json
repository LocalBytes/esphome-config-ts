{
  "qr_code": {
    "schemas": {
      "CONFIG_SCHEMA": {
        "type": "schema",
        "schema": {
          "config_vars": {
            "id": {
              "key": "Required",
              "id_type": {
                "class": "qr_code::QrCode",
                "parents": [
                  "Component"
                ]
              }
            },
            "value": {
              "key": "Required",
              "type": "string"
            },
            "ecc": {
              "key": "Optional",
              "default": "LOW",
              "type": "enum",
              "values": {
                "LOW": null,
                "MEDIUM": null,
                "QUARTILE": null,
                "HIGH": null
              }
            }
          }
        },
        "is_list": true
      }
    }
  }
}