{
  "power_supply": {
    "schemas": {
      "CONFIG_SCHEMA": {
        "type": "schema",
        "schema": {
          "config_vars": {
            "id": {
              "key": "Required",
              "id_type": {
                "class": "power_supply::PowerSupply",
                "parents": [
                  "Component"
                ]
              }
            },
            "pin": {
              "key": "Required",
              "schema": true,
              "modes": [
                "output"
              ],
              "type": "pin"
            },
            "enable_time": {
              "key": "Optional",
              "default": "20ms",
              "type": "schema",
              "schema": {
                "extends": [
                  "core.positive_time_period_milliseconds"
                ]
              }
            },
            "keep_on_time": {
              "key": "Optional",
              "default": "10s",
              "type": "schema",
              "schema": {
                "extends": [
                  "core.positive_time_period_milliseconds"
                ]
              }
            }
          },
          "extends": [
            "core.COMPONENT_SCHEMA"
          ]
        },
        "is_list": true
      }
    }
  }
}