{
  "rgbct.light": {
    "schemas": {
      "CONFIG_SCHEMA": {
        "type": "schema",
        "schema": {
          "extends": [
            "light.RGB_LIGHT_SCHEMA"
          ],
          "config_vars": {
            "output_id": {
              "key": "GeneratedID",
              "id_type": {
                "class": "rgbct::RGBCTLightOutput",
                "parents": [
                  "light::LightOutput"
                ]
              }
            },
            "red": {
              "key": "Required",
              "use_id_type": "output::FloatOutput",
              "type": "use_id"
            },
            "green": {
              "key": "Required",
              "use_id_type": "output::FloatOutput",
              "type": "use_id"
            },
            "blue": {
              "key": "Required",
              "use_id_type": "output::FloatOutput",
              "type": "use_id"
            },
            "color_temperature": {
              "key": "Required",
              "use_id_type": "output::FloatOutput",
              "type": "use_id"
            },
            "white_brightness": {
              "key": "Required",
              "use_id_type": "output::FloatOutput",
              "type": "use_id"
            },
            "cold_white_color_temperature": {
              "key": "Required"
            },
            "warm_white_color_temperature": {
              "key": "Required"
            },
            "color_interlock": {
              "key": "Optional",
              "default": "False",
              "type": "boolean"
            }
          }
        }
      }
    }
  }
}